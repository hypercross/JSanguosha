	public String[] names;
	
	public void query(City a,City b)
	{
		cost = Integer.MAX_VALUE;
		nodes = 0;
		time = System.nanoTime();
		
		searchList.clear();
		searchList.add(a);
		finishList.clear();
		
		while(searchList.)
	}
	
}
