package application.network;

import game.entity.PlayerEntity;

public class PlayerEntityUpdate extends EntityUpdate<PlayerEntity> {
	int hp,maxHp,seatId;
	boolean isAlive;
	
	public void update(PlayerEntity entity)
	{
		super.update(entity);
		entity.hp = hp;
		entity.maxHp = maxHp;
		entity.seatId = seatId;
	}
}
