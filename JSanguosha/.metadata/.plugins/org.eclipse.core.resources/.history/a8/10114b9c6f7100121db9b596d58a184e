package application.network;

import game.entity.CardEntity;

public class CardEntityUpdate extends EntityUpdate<CardEntity>{
	public int cardID;
	public byte number;
	public char suit;
	
	@Override
	public void update(CardEntity ce)
	{
		super.update(ce);
		ce.cardID = cardID;
		ce.number = number;
		ce.suit = suit;
	}
}
